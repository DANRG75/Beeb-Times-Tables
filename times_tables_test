1 MODE 7:VDU23;11;0;0;0;
5 PRINT CHR$(132);CHR$(157);CHR$(131);CHR$(141);"        TIMES TABLES TEST"
6 PRINT CHR$(132);CHR$(157);CHR$(131);CHR$(141);"        TIMES TABLES TEST"
7 VDU 28,1,20,32,3
10 A% = RND(12)
20 B% = RND(12)
30 C% = A% * B%
40 PRINT CHR$(131);"What is";CHR$(RND(6)+129);A%;CHR$(135);"x";CHR$(RND(6)+129);B%;CHR$(135);
50 INPUT ANS%
60 IF ANS% = C% THEN PRINT CHR$(RND(6)+129);C%;" is correct! Well done!":GOTO 10
70 ELSE 
80 PRINT CHR$(131);"That's not right!"
90 GOTO 40